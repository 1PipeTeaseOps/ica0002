global:
  scrape_interval: 15s
  evaluation_interval: 15s

scrape_configs:
  - job_name: 'prometheus'
    scrape_interval: 5s
    scrape_timeout: 5s
    metrics_path: /prometheus/metrics
    static_configs:
      - targets:
          - '1PipeTeaseOps-2:9090'

  - job_name: 'linux'
    static_configs:
      - targets:
          {% for host in groups['all'] %}
          - '{{ host }}:9100'
          {% endfor %}
